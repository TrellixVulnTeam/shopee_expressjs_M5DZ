<div class="grid">
    <div class="row">
    <div class="col-md-12 col-md-offset-2">
        <h1>My order</h1>
        <hr>
        {{# each orders }}
        <div class="panel panel-default">
            <div class="panel-body">
                <ul class="list-group">
                    {{# each this.items }}
                    <li class="list-group-item">
                        {{this.item.description}} | {{this.qty}} Units
                        <span class="badge badge-secondary">{{this.price}}</span>
                    </li>
                    {{/each}}
                </ul>
            </div>
            <div class="panel-footer"> {{this.cart.totalPrice}}Ä‘</div>
        </div>
        {{/each}}
    </div>
</div>
</div>